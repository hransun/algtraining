https://leetcode-cn.com/problems/sqrtx/submissions/

class Solution(object):
    def mySqrt(self, x):
        """
        :type x: int
        :rtype: int
        """
        left , right = 0 , x
        while left + 1 < right:
            mid = (left + right) /2
            if mid * mid == x:
                return mid
            elif mid * mid < x :
                left = mid   
           
            else:
                right = mid
        
        if right * right <= x:
            return right
        if left * left <= x :
            return left 