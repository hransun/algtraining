https://leetcode-cn.com/problems/container-with-most-water/submissions/

class Solution(object):
    def maxArea(self, height):
        """
        :type height: List[int]
        :rtype: int
        """
        ans =0
        i , j = 0 , len(height) -1
        while i < j:
            ans = max(ans, (min(height[i], height[j]))* (j - i))
            if height[i] < height[j]:
                i+=1
            else:
                j-=1
        return ans 
        